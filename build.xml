<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Spacetraveler">
    <!--this file was created by Eclipse Runnable JAR file Export Wizard-->
    <!--ANT 1.7 is required-->
    <!--define folder properties-->
    <target name="create_run_jar">
        <jar destfile="bin/spacetraveler.jar">
            <manifest>
                <attribute name="Main-Class" value="org.eclipse.jdt.internal.jarinjarloader.JarRsrcLoader"/>
                <attribute name="Rsrc-Main-Class" value="spacetraveler.Game"/>
                <attribute name="Class-Path" value="."/>
                <attribute name="Rsrc-Class-Path" value="./ jsfml.jar"/>
            </manifest>
            <zipfileset src="lib/jar-in-jar-loader.zip"/>
            <fileset dir="bin/"/>
            <zipfileset dir="lib/" includes="jsfml.jar"/>
        </jar>
    </target>
</project>
